<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Map of custom Change Request statuses to the three standard status values (Not Started, In Progress, Completed). Defaults to the standard, out-of-the-box mapping. Can be modified by the customer.</description>
    <fields>
        <fullName>FLODocs__Order__c</fullName>
        <deprecated>false</deprecated>
        <description>The order that the status values appear in lists.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>The order that the status values appear in lists.</inlineHelpText>
        <label>Order</label>
        <precision>4</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>FLODocs__Standard_CR_Status__c</fullName>
        <deprecated>false</deprecated>
        <description>The three standard Change Request statuses.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>The three standard Change Request statuses.</inlineHelpText>
        <label>Standard CR Status</label>
        <required>true</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Not Started</fullName>
                    <default>true</default>
                    <label>Not Started</label>
                </value>
                <value>
                    <fullName>In Progress</fullName>
                    <default>false</default>
                    <label>In Progress</label>
                </value>
                <value>
                    <fullName>Completed</fullName>
                    <default>false</default>
                    <label>Completed</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <label>Custom CR Status Map</label>
    <listViews>
        <fullName>FLODocs__Custom_to_Standard_Status_Map</fullName>
        <columns>MasterLabel</columns>
        <columns>DeveloperName</columns>
        <columns>FLODocs__Standard_CR_Status__c</columns>
        <columns>FLODocs__Order__c</columns>
        <filterScope>Everything</filterScope>
        <label>Custom-to-Standard Status Map</label>
    </listViews>
    <pluralLabel>Custom CR Status Maps</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
