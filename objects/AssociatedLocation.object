    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <enableFeeds>false</enableFeeds>
    <externalSharingModel>ControlledByParent</externalSharingModel>
    <fields>
        <fullName>ActiveFrom</fullName>
        <trackHistory>true</trackHistory>
    </fields>
    <fields>
        <fullName>ActiveTo</fullName>
        <trackHistory>true</trackHistory>
    </fields>
    <fields>
        <fullName>Active_Address__c</fullName>
        <externalId>false</externalId>
        <formula>ISNULL(ActiveTo)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>To deactivate the address, please populate &apos;Active To&apos;</inlineHelpText>
        <label>Active Address</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Alternate_Address_Name__c</fullName>
        <description>Provides an alternative Address name for the associated location</description>
        <externalId>false</externalId>
        <inlineHelpText>This Location Name will show on the release slip instead of Dealer Name if this address is selected for Transportation</inlineHelpText>
        <label>Alternate Address Name</label>
        <length>128</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AssociatedLocationNumber</fullName>
        <trackHistory>false</trackHistory>
    </fields>
    <fields>
        <fullName>Associated_Location_Guid__c</fullName>
        <caseSensitive>true</caseSensitive>
        <description>Unique Guid of this associated location</description>
        <externalId>true</externalId>
        <label>Associated Location Guid</label>
        <length>36</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Auto_Inc_Id__c</fullName>
        <description>Auto Increment Id used to generate Org address Ids</description>
        <externalId>false</externalId>
        <label>Auto Inc Id</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </fields>
    <fields>
        <fullName>From_Historical_Load__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Indicates this record was created through a historical load</description>
        <externalId>false</externalId>
        <label>From Historical Load</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Full_Location_Address__c</fullName>
        <description>This is the entire address</description>
        <externalId>false</externalId>
        <formula>Location.VisitorAddress.Street &amp;&quot; &quot;&amp;  Location.VisitorAddress.City &amp;&quot;, &quot;&amp; Location.VisitorAddress.State &amp;&quot; &quot;&amp; Location.VisitorAddress.PostalCode &amp;&quot; &quot;&amp; Location.VisitorAddress.Country</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Full Location Address</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Inherited_Address_Id__c</fullName>
        <defaultValue>false</defaultValue>
        <description>This field will populate when an address is being changed (ie: old associatedLocation is being de-activated and Org Address ID/guid is being transferred) creating a NEW associatedLocation with the org Address ID/guid of the old associatedLocation</description>
        <externalId>false</externalId>
        <label>Inherited Address Id</label>
        <trackHistory>true</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>LocationId</fullName>
        <inlineHelpText>To deactivate the address, please populate &apos;Active To</inlineHelpText>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Notes__c</fullName>
        <description>Notes related to the relationship between this Account and the Address association</description>
        <externalId>false</externalId>
        <label>Notes</label>
        <length>2000</length>
        <trackHistory>true</trackHistory>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Organization_Address_Guid__c</fullName>
        <caseSensitive>true</caseSensitive>
        <description>Unique Guid associated with an Organization and an address, along with the unique address type</description>
        <externalId>true</externalId>
        <label>Organization Address Guid</label>
        <length>36</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Organization_Address_Id__c</fullName>
        <description>This field is the unique Id between the Organizaition and the Address associated with it, along with the unique Address type</description>
        <externalId>true</externalId>
        <label>Organization Address Id</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>true</trackHistory>
        <type>Number</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>ParentRecordId</fullName>
        <trackHistory>false</trackHistory>
        <type>MasterDetail</type>
    </fields>
    <fields>
        <fullName>Type</fullName>
        <trackHistory>true</trackHistory>
        <type>Picklist</type>
    </fields>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Cannot_Change_Location_Ref</fullName>
        <active>true</active>
        <description>After an associated Location&apos;s Location is created it cannot be changed</description>
        <errorConditionFormula>IsChanged(LocationId)</errorConditionFormula>
        <errorMessage>Cannot Change Location after Associated Location has been updated. Please de-activate this Associated Location and Create a new one</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Type</fullName>
        <active>true</active>
        <description>Associated Location Type cannot be changed</description>
        <errorConditionFormula>IsChanged(Type)</errorConditionFormula>
        <errorMessage>Type cannot be changed. Please De-activate this Address if it is no longer valid and recreate a new address with the correct type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Delete_ActiveTo</fullName>
        <active>true</active>
        <description>After an associated Location&apos;s Active To is filled, it cannot be deleted.</description>
        <errorConditionFormula>AND( 
ISCHANGED(ActiveTo), 
ISNULL(ActiveTo),
NOT(ISNULL(PRIORVALUE(ActiveTo))))</errorConditionFormula>
        <errorMessage>Cannot Delete Active To after Associated Location has been updated. To reactivate the address, Please create a new one</errorMessage>
    </validationRules>
</CustomObject>
