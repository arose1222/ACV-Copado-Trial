    <validationRules>
        <fullName>AL_Sales_Tax_must_have_AL_and_Resale</fullName>
        <active>false</active>
        <description>If AL_Sales_Tax field is true, State must be AL AND Type must be Resale Cert.</description>
        <errorConditionFormula>AND(
AL_Sales_Tax__c = true, 
OR(
Text(Document_State__c) != &apos;AL&apos;, 
RecordType.DeveloperName != &apos;Resale_Certificate&apos;))</errorConditionFormula>
        <errorMessage>Can only select AL Sales Tax if the Document State is AL and the Document Type is a Resale Certificate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_not_mark_No_Expiration_with_Date</fullName>
        <active>false</active>
        <description>Throws an error if user sets &quot;No Expiration&quot; to true and they have an Expiration Date filled in.</description>
        <errorConditionFormula>AND(No_Expiration__c = True, NOT(ISNULL(Expiration_Date__c)))</errorConditionFormula>
        <errorDisplayField>Expiration_Date__c</errorDisplayField>
        <errorMessage>You can not input an Expiration Date if the No Expiration Date field is set to true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_have_AL_Sales_Tax_AND_MTC_true</fullName>
        <active>false</active>
        <description>AL_Sales_Tax__c and MTC__c cannot both be marked as true.</description>
        <errorConditionFormula>AND(AL_Sales_Tax__c = true, MTC__c = true)</errorConditionFormula>
        <errorMessage>Cannot have both AL Sales Tax and MTC selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirmation_Doc_Not_Editable</fullName>
        <active>false</active>
        <description>Make sure no one can edit Confirmation Docs except admins and anyone with special permission.</description>
        <errorConditionFormula>AND(
$Profile.Name != &quot;System Administrator&quot;,
NOT($Permission.Bypass_Dealer_Doc_Lockdown),
RecordType.DeveloperName = &apos;License_Renewal_Confirmation&apos;,
OR(
ISCHANGED(Account__c),
ISCHANGED(License_Number__c),
ISCHANGED(Expiration_Date__c),
ISCHANGED(Related_Dealer_License__c))
)</errorConditionFormula>
        <errorMessage>This document should not need its information changed after creation. Please submit a salesforce Requests &amp; Inquiries ticket if you need something changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Docs_Not_Editable_After_Marked_Compliant</fullName>
        <active>false</active>
        <description>Certain fields on Docs should not be changed after the doc has been marked as compliant. Admins will still be able to edit.</description>
        <errorConditionFormula>OR(
AND(
$Profile.Name != &quot;System Administrator&quot;,
NOT($Permission.Bypass_Dealer_Doc_Lockdown),
Compliant__c = true,
PRIORVALUE(Compliant__c) = true, 
(OR(
ISCHANGED(Account__c),
ISCHANGED(Document_State__c),
ISCHANGED(License_Number__c),
ISCHANGED(Permit_Resale_Number__c),
ISCHANGED(Expiration_Date__c),
ISCHANGED(No_Expiration__c),
ISCHANGED(AL_Sales_Tax__c),
ISCHANGED(MTC__c)
))
),
AND(
$Profile.Name != &quot;System Administrator&quot;,
NOT($Permission.Bypass_Dealer_Doc_Lockdown),
Compliant__c = false,
ISCHANGED(Compliant__c)
)
)</errorConditionFormula>
        <errorMessage>Existing, compliant document records should not be edited.  Please submit a Requests &amp; Inquiries/Salesforce Request ticket, marked as &apos;Error message or bug&apos;, and an admin will review your proposed edits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Document_Type</fullName>
        <active>false</active>
        <description>Rule that flows through the possible document types, depending on the document type corresponding fields will need to be filled out.</description>
        <errorConditionFormula>IF(
OR(
RecordType.DeveloperName = &apos;Dealer_License&apos;,
RecordType.DeveloperName = &apos;License_Renewal_Confirmation&apos;) 
&amp;&amp; License_Number__c = null , 
TRUE, 
IF(
RecordType.DeveloperName = &apos;Resale_Certificate&apos; 
&amp;&amp;  Permit_Resale_Number__c = null 
&amp;&amp; NOT(
INCLUDES(
Compliance_Issues__c, 
&apos;Missing Resale/Tax ID #&apos;)) , 
TRUE, 
IF(
RecordType.DeveloperName = &apos;Dealer_License&apos; 
&amp;&amp; Permit_Resale_Number__c != null, 
TRUE, 
IF(
RecordType.DeveloperName = &apos;Resale_Certificate&apos; 
&amp;&amp;  License_Number__c != Null, 
TRUE, 
False))))</errorConditionFormula>
        <errorMessage>A Dealer Doc that&apos;s a Dealer License must have a value in the License Number field and NO values in the Permit/Resale Number. A Dealer Doc that&apos;s a Resale Certificate must have a value in the Permit/Resale Number and NO value in the License Number field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExpDate_Cannot_be_in_Past_if_Compliant</fullName>
        <active>false</active>
        <description>Cannot have an expiration date in the past if the document is compliant.</description>
        <errorConditionFormula>AND(
( Expiration_Date__c &lt; TODAY()),
( Compliant__c = TRUE)
)</errorConditionFormula>
        <errorMessage>Cannot have an expiration date in the past if the document is compliant. Please enter the correct expiration date or do not mark this record as compliant.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expiration_Date_Validation</fullName>
        <active>false</active>
        <description>If Document Type = Resale Certificate, and Document State = AZ, MI, NE, WA, ME, LA, IA, IL, or FL, force user to enter data in the Expiration Date field.</description>
        <errorConditionFormula>OR(
AND(
RecordType.DeveloperName = &apos;Resale_Certificate&apos;,  
MTC__c == False,
OR(
TEXT(Document_State__c)= &apos;AZ&apos;,
TEXT(Document_State__c)= &apos;MI&apos;,
TEXT(Document_State__c) =&apos;NE&apos;,
TEXT(Document_State__c)= &apos;WA&apos;,
TEXT(Document_State__c) =&apos;FL&apos;, 
TEXT(Document_State__c)=&apos;ME&apos;,
TEXT(Document_State__c)=&apos;LA&apos;,
TEXT(Document_State__c)=&apos;IA&apos;,
TEXT(Document_State__c)=&apos;IL&apos;),
ISNULL(Expiration_Date__c)),
AND(
ISNULL(Expiration_Date__c), 
AL_Sales_Tax__c == True))</errorConditionFormula>
        <errorDisplayField>Expiration_Date__c</errorDisplayField>
        <errorMessage>You must enter an expiration date. If no expiration date is listed, it is this far out: AL/FL = 1yr, AZ/MI/WA = 4yrs, ME/LA/NE/IA/IL = 3yrs. Ex: Document state = AZ; signature date = 2/28/19; expiration date = 2/28/23.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fill_out_Expiration_Field</fullName>
        <active>false</active>
        <description>If Document Type = Dealer License, force user to enter data in the Expiration Date field. Error msg: &quot;You must enter an expiration date for this document.&quot;</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName = &apos;Dealer_License&apos;,
ISNULL(Expiration_Date__c),
AND(
TEXT(Document_State__c) != &apos;AZ&apos;,
TEXT(Document_State__c) != &apos;VT&apos;)
)</errorConditionFormula>
        <errorDisplayField>Expiration_Date__c</errorDisplayField>
        <errorMessage>You must enter an expiration date for this document.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fill_out_No_Expiration_Field</fullName>
        <active>false</active>
        <description>If Document Type = Resale Certificate, and Document State not equal to IL, AZ, LA, ME, MI, NE, WA or FL, force user to select &quot;true&quot; on the No Expiration field. Error msg: &quot;Please mark the &apos;No Expiration&apos; field as true for this record.&quot;</description>
        <errorConditionFormula>OR(
AND(
RecordType.DeveloperName = &apos;Resale_Certificate&apos;, 
MTC__c == false,
TEXT(Document_State__c) != &apos;IL&apos;,
TEXT(Document_State__c) != &apos;AZ&apos;,
TEXT(Document_State__c) != &apos;MI&apos;,
TEXT(Document_State__c) != &apos;NE&apos;,
TEXT(Document_State__c) != &apos;WA&apos;,
TEXT(Document_State__c) != &apos;FL&apos;,
TEXT(Document_State__c) != &apos;ME&apos;,
TEXT(Document_State__c) != &apos;LA&apos;,
TEXT(Document_State__c) != &apos;AL&apos;,
No_Expiration__c = FALSE),
AND(
RecordType.Name = &apos;Resale Certificate&apos;, 
No_Expiration__c = FALSE, 
TEXT(Document_State__c) = &apos;AL&apos;, 
AL_Sales_Tax__c = false))</errorConditionFormula>
        <errorDisplayField>No_Expiration__c</errorDisplayField>
        <errorMessage>Please mark the &apos;No Expiration&apos; field as true for this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Compliant_True_no_Compliance_issue</fullName>
        <active>false</active>
        <description>Validation rule that ensures if the Complaint checkbox is true there are no values selected in the Compliance Issues picklist.</description>
        <errorConditionFormula>AND( Compliant__c =True, ISNULL(Compliance_Issues__c )!=True)</errorConditionFormula>
        <errorDisplayField>Compliance_Issues__c</errorDisplayField>
        <errorMessage>Error message: A document cannot be marked as compliant and contain compliance issues. Please check your work and re-save. If you have any questions, please contact Sue Engasser.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MTC_Field</fullName>
        <active>false</active>
        <description>Rule #1: Do NOT allow the MTC field to be marked as &apos;true&apos; for the following states:
AK, DE, DC, IN, LA, MA, MS, MT, NH, NY, OR, PR, VA, WV, WY</description>
        <errorConditionFormula>IF(MTC__c == true &amp;&amp; 
   
OR(TEXT(Document_State__c) =&apos;AK&apos;,
TEXT(Document_State__c) =&apos;DE&apos;,
TEXT(Document_State__c) =&apos;DC&apos;,
TEXT(Document_State__c) =&apos;IN&apos;,
TEXT(Document_State__c) =&apos;LA&apos;,
TEXT(Document_State__c) =&apos;MA&apos;,
TEXT(Document_State__c) =&apos;MS&apos;,
TEXT(Document_State__c) =&apos;MT&apos;,
TEXT(Document_State__c) =&apos;NH&apos;,
TEXT(Document_State__c) =&apos;NY&apos;,
TEXT(Document_State__c) =&apos;OR&apos;,
TEXT(Document_State__c) =&apos;PR&apos;,
TEXT(Document_State__c) =&apos;VA&apos;,
TEXT(Document_State__c) =&apos;WV&apos;,
TEXT(Document_State__c) =&apos;WY&apos;), True, False)</errorConditionFormula>
        <errorDisplayField>MTC__c</errorDisplayField>
        <errorMessage>The state selected does not allow for the use of MTC. Please request a state supplied resale certificate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MTC_is_True</fullName>
        <active>false</active>
        <description>Use of MTC in this state has expiration: IL and NE = 3 years from signature; MI and WA = 4 years from signature.</description>
        <errorConditionFormula>IF(
AND(
RecordType.DeveloperName = &apos;Resale_Certificate&apos;,  
MTC__c == false), False,
If(
AND(
MTC__c == true, 
ISNULL(Expiration_Date__c)) &amp;&amp;
OR(
TEXT(Document_State__c) =&apos;IL&apos;,
TEXT(Document_State__c) =&apos;MI&apos;, 
TEXT(Document_State__c) =&apos;NE&apos;, 
TEXT(Document_State__c) =&apos;WA&apos;), 
TRUE, 
FALSE))</errorConditionFormula>
        <errorMessage>Use of MTC in this state has expiration: IL and NE = 3 years from signature; MI and WA = 4 years from signature. Please enter an expiration date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Review_Status_Complete</fullName>
        <active>false</active>
        <description>Review status can not be marked as complete unless the Complaint check box is marked to true.</description>
        <errorConditionFormula>AND(TEXT(Review_Status__c) = &apos;Complete&apos;,  Compliant__c == false)</errorConditionFormula>
        <errorDisplayField>Compliant__c</errorDisplayField>
        <errorMessage>You may not mark this Review Status to complete until this Dealer Doc is marked to compliant.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
