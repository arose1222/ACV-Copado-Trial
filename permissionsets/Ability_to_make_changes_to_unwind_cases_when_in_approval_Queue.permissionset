<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Needed access to edit unwinds when they are in approval queue for payments.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Ability to make changes to unwind cases when in approval Queue</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>true</modifyAllRecords>
        <object>Case</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <recordTypeVisibilities>
        <recordType>Case.Title_Unwind</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
</PermissionSet>