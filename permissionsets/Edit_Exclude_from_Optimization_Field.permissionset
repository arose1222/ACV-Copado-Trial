<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Allows someone to edit the &quot;Exclude from Optimization&quot; field on Service Appointment and see the &quot;Display Latitude&quot; and &quot;Display Longitude&quot; fields</description>
    <fieldPermissions>
        <editable>false</editable>
        <field>ServiceAppointment.Display_Latitude__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>false</editable>
        <field>ServiceAppointment.Display_Longitude__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>ServiceAppointment.Exclude_from_Optimization__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Edit Exclude from Optimization Field</label>
</PermissionSet>