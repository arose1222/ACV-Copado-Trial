<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Users with this permission set will be able to set system fields such as Created and Last Modified Date when inserting new records through methods such as data loader.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Set Audit Fields upon Record Creation</label>
    <license>Salesforce</license>
    <userPermissions>
        <enabled>true</enabled>
        <name>CreateAuditFields</name>
    </userPermissions>
</PermissionSet>