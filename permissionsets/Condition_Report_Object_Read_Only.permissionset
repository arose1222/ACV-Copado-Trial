<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Created to give access to the condition report object in read only mode - original use case was for Josh Raby to conduct a pilot for Vehicle intelligence.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Condition Report Object Read Only</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>false</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Condition_Report__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
</PermissionSet>