<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This permission set will allow users to delete case records with the Arbitration record type.  used for deleting duplicate case records.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Arbitration Case Delete</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Case</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <recordTypeVisibilities>
        <recordType>Case.Arbitration_Claim</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
</PermissionSet>