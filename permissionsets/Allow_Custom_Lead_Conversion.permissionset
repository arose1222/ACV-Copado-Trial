    </classAccesses>
    <description>This permission set provides access to new Lead record type and allows users to convert the lead.</description>
    <fieldPermissions>
        <editable>true</editable>
        <field>Lead.Dont_Send_BDR_Email__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Lead.Interested_In_Selling__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Lead.Interested_in_Buying__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Allow Custom Lead Conversion</label>
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>true</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>true</modifyAllRecords>
        <object>Lead</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <recordTypeVisibilities>
        <recordType>Lead.Organization_Lead</recordType>
        <visible>true</visible>
    </recordTypeVisibilities>
