          //****
          function ValidateBusinessInformation()
          {
              var isValidBI = true; 
              if(j$('input[id$="txtBIN"]').val().trim() == '')
              {    
                  if(j$('[id$=slsTab1SearchCountry]').val() == ''){
                      j$('#spnTab1SearchCountryMsg').show();                      
