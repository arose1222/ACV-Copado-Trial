<apex:page controller="jiffle.JNBookMeeting" standardStylesheets="false" showHeader="true" sidebar="false"
tabStyle="Jifflenow_Meetings__tab">
<link rel="stylesheet" href="{!URLFOR($Resource.JNResources, 'JNResources/application.css')}" />
<html lang="en">
<apex:include pageName="jiffle__JNConsent" />
<apex:include pageName="jiffle__JNGlobals" />
<div id="sf-jiffle-now" class="jiffle-book-meeting iris-bootstrap hide">
  <div id="meeting-request-container" class="hide">
    <div class="mount-meeting-summary"></div>
    <div class="bm-page">
      <input id="ac-1" class="instruction-check hide" name="accordion-1" type="checkbox" autocomplete="off" />
      <div class="instruction-container" id="js-instructions-display"></div>
      <label class="instruction-label hide" for="ac-1">
        <i class="jif-info"></i>
      </label>
      <div class="meeting-detail-container">
        <div class="precond-head">
          <div class="row">
            <div class="col-xs-9 col-sm-6 col-md-9">
              <form action="javascript:void(0)" class="jn-form-inline precond-form" role="form" id="details-form">
                <div class="row">
                  <div class="col-xs-6 hide">
                    <div id="account-opportunity">
                      <div class="row">
                        <div class="col-xs-6">
                          <div class="form-group account precond-topic">
                            <label class="js_tooltip" data-placement="bottom" data-container='#sf-jiffle-now'>Account</label>
                            <div class="jn-select2">
                              <select name="select-boxes" id="select_account" class="form-control"
                              style="width: 100%;"></select>
                            </div>
                          </div>
                        </div>
                        <div class="col-xs-6">
                          <div class="form-group opportunity precond-topic">
                            <label class="js_tooltip" data-placement="bottom" data-container='#sf-jiffle-now'>Opportunity</label>
                            <div class="jn-select2">
                              <select name="select-boxes" id="select_opportunity" class="form-control" style="width: 100%;"></select>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-xs-3">
                    <div class="js-meeting-with precond-meetingwith form-group" id="req-page-meeting-form">
                      <label>Meeting with</label>
                      <input type="textfield" name="meeting_with" id="req_page_meeting_with" class="form-control"/>
                    </div>
                  </div>
                  <div class="col-xs-2">
                    <div class="form-group precond-requestor">
                      <label>Requestor:</label>
                      <div class="jn-select2">
                        <select class="form-control" name="requestor_list" id='requestor-select-complement'
                        required="required">
                        </select>
                        <div id="requestor-label-div-complement" class="requestor-label-div hide"></div>
                      </div>
                    </div>
                  </div>
                  <div class="col-xs-2">
                    <div class="form-group">
                      <label>Meeting Type</label>
                      <div class="precond-meetingtype overflow-ellipsis js-activity-name requestor-label-div">
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
            <div class="col-xs-3 col-sm-6 col-md-3">
              <div class="action-btns text-right">
                <button class="btn btn-tertiary delete-btn" id='cancel-btn'>Discard</button>
                <button class="btn blue-btn" id='proceed-btn' data-i18n="proceed"></button>
                <button class="btn blue-btn hide" id='quick_submit_btn' data-i18n="quick_book"></button>
              </div>
            </div>
          </div>
        </div>
        <div class="modal select-time-modal mapping-time-popup" id='custom_popup' tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-body">
                <div class="select-time">
                  <div id='error_block' style="color:red"></div>
                  <form class="jiffle-form">
                    <div class="row">
                      <label class="font-semi-bold pull-left margin-top-4 bottom-margin-10"
                      data-i18n="set_time"></label>
                      <span class="pull-right point-cursor" id="custom_popup_close">
                        <i class="jif-iris-cancel-thin jif-8-10"></i>
                      </span>
                      <div class="col-xs-12">
                        <div class="form-group" data-form-i18n="start_time">
                          <label>Start</label>
                          <input type="text" class="form-control" placeholder="HH:MM" id='start_time' />
                        </div>
                      </div>
                      <div class="col-xs-12">
                        <div class="form-group" data-form-i18n="end_time">
                          <label>End</label>
                          <input type="text" class="form-control" placeholder="HH:MM" id='end_time' />
                        </div>
                      </div>
                      <div class="col-xs-12">
                        <a href="javascript:void(0)" class="btn btn-blue" id="custom_popup_submit"
                        data-i18n="Confirm"></a>
                        <a href="javascript:void(0)" class="font-blue font-10" id="custom_popup_unselect">Discard</a>
                      </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="con-section">
        <div class='con-tab'>
          <ul id="meetingList"></ul>
          <button class="btn grey-btn-round js_tooltip" title="Add Meeting" data-placement="top" id='consecutive_add_meeting_btn' >+</button>
        </div>
        <div class="con-contents">
          <input id="ac-1" class="instruction-check hide" name="accordion-1" type="checkbox" />
          <div class="instruction-container" id="js-instructions-display"></div>
          <label class="instruction-label hide" for="ac-1"> <i class="jif-info"></i></label>
          <div class="con-header">
            <form id="topic-details" class="form-inline">
              <div class="form-group">
                <label class="text-uppercase required"></label>
                <select type="textfield" name="select_meeting_type" class="form-control form-element-ignorable" id="select_meeting_type" ></select>
              </div>
              <div class="form-group" id="req-page-topic-management">
                <label class="js_tooltip" data-placement="bottom"></label>
                <select type="textfield" name="topic_management" class="form-control form-element-ignorable" id="topic_management-complement"></select>
                <i class="jif-iris-info-filled" id="topic-description" data-original-title="" title=""></i>
              </div>
            </form>
            <div class="delete-btn icon-box js-delete-current-meeting">
              <i class="jif-iris-trash"></i>
              <div id="js_delete_label"></div>
            </div>
          </div>
          <div id='meeting-empty-state' class="con-emptystate">
            <div class="con-emptystate-icon"></div>
            <div class="con-emptystate-msg">Attendees, Time and Location are loaded once you select a meeting type</div>
          </div>
          <div class="bm-container js-meeting-entities">
            <div class="bm-attendee-col">
              <div class="section attendees-section">
                <div class="section-header">
                  <span class="section-title pull-left" data-i18n="pick_participants">Pick Attendees</span>
                  <div class="section-actions">
                    <i class="jif-iris-add js_tooltip" title="Add Attendee" data-placement="top" id="attendee-addbtn" data-target="#add-attendee-modal" data-toggle="modal"></i>
                    <i class="copy-customer-info jif-iris-copy js-copy-attendee js_tooltip" title="Copy Attendee" data-placement="top"></i>
                  </div>
                </div>
                <div class="section-tabs">
                  <ul class="nav nav-tabs tabs-attendees" role="tablist">
                    <li role="presentation" class="active" data-search-name='internal'>
                      <a href="#internal-attendees-tab" role="tab" data-toggle="tab" id='internal'><span data-i18n="internal">Internal</span>&nbsp;<span class="count attendeeCount" id="internalAttendeeCount">0</span></a>
                    </li>
                    <li role="presentation" data-search-name='external'>
                      <a href="#external-attendees-tab" role="tab" data-toggle="tab" id='external'><span data-i18n="external">External</span>&nbsp;<span class="count attendeeCount" id="externalAttendeeCount">0</span></a>
                    </li>
                  </ul>
                </div>
                <div class="pick-tool">
                  <div class="search-field-icon-left search-field-container jif-dark2-grey">
                    <i id='search-field-context-icon-internal' class='jif-12 jif-iris-search contextual-field-icon js_clear-search'></i>
                    <i id='search-field-context-icon-external' class='jif-12 jif-iris-search contextual-field-icon js_clear-search'></i>
                    <input type="text" class="form-control search-box user-search-box" />
                  </div>
                  <ul class="nav navbar-nav navbar-right navbar-icon hide" id='attendee-filter-panel'>
                    <li class="dropdown filter-dropdown">
                      <a href="javascript:void(0)" class="filter-dropdown-toggle" data-toggle="dropdown"></a>
                      <div class="dropdown-menu">
                        <form action="javascript:void(0)" class="form">
                          <div class="form-group-search">
                            <div class="row">
                              <div class="col-sm-10">
                                <input type="text" class="form-control" id='attendee-filter-search' />
                              </div>
                              <div class="col-sm-2"></div>
                            </div>
                          </div>
                          <span data-i18n="filter_by"></span>
                          <div class="form-group">
                            <div class="row" id='attendee-filter'></div>
                          </div>
                          <div class="text-left">
                            <a href="javascript:void(0)" class="cancel pull-left" data-i18n="reset"></a>
                            <button class="btn btn-blue pull-right apply" data-i18n="apply_filter"></button>
                          </div>
                        </form>
                      </div>
                    </li>
                  </ul>
                </div>

                <div class="section-content">
                  <div class="tab-content tabs-attendees-content">
                    <div role="tabpanel" class="tab-pane active" id="internal-attendees-tab">
                      <div class="added-intetrnal-attendee"></div>
                      <div class="attendees-list"></div>
                      <div class="loading hide">
                        <span><b data-i18n="loading"></b></span>
                      </div>
                    </div>
                    <div role="tabpanel" class="tab-pane" id="external-attendees-tab">
                      <div class="added-external-attendee"></div>
                      <div class="attendees-list"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <div class="bm-calendar-col book-meeting">
              <div class="js-date-picker-section date-picker-section jiffle-form"></div>
              <div class="section calendar-section">
                <div class="section-header">
                  <div class="header-left pull-left">
                    <span class="section-title calendar-section-title" data-i18n="select_meeting"></span>
                    <span class="cal-help-text" data-i18n="select_hold_drag"></span>
                  </div>
                </div>
                <div class="section-caltime">
                  <form class="js-set-time-form caltime-form">
                    <div class="section-caltime-left">
                      <span>From</span>
                      <div class="section-caltime-bx">
                        <input type="text" name="start_time" readonly="true"></input>
                      </div>
                      <span>To</span>
                      <div class="section-caltime-bx">
                        <input type="text" name="end_time" readonly="true"></input>
                      </div>
                    </div>
                    <div class='pull-left'>
                      <i class='js-clear-cal-event jif-iris-trash point-cursor'></i>
                    </div>
                    <div id="error_block" class="caltime-error" style="color: #a94442"></div>
                  </form>
                </div>
                <div class="section-content cal-content">
                  <div class="text-center hidden-lg hidden-md hidden-sm" id="timeLabel"></div>
                  <div class="calender">
                    <div id="scheduler_here" class="dhx_cal_container">
                      <div class="nav-buttons">
                        <ul class="list-inline">
                          <li class="js-calendar-dropleft">
                            <a href="javascript:void(0)"><i class="jif-iris-dropleft"></i></a>
                          </li>
                          <li class="js-calendar-dropright">
                            <a href="javascript:void(0)"><i class="jif-iris-dropright"></i></a>
                          </li>
                        </ul>
                      </div>
                      <div class="dhx_cal_navline hide" >
                        <div class="dhx_cal_prev_button">&nbsp;</div>
                        <div class="dhx_cal_next_button">&nbsp;</div>
                        <div class="dhx_cal_date hide"></div>
                      </div>
                      <div class="dhx_cal_header"></div>
                      <div class="dhx_cal_data"></div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="bm-cal-legend">
                <div class="legend-show" id="legend_expand">
                  <i class="jif-iris-cancel-thin" id="legend_close"></i>
                  <div class="bm-legend">
                    <div class="bm-slot-unavail-legend">Slot unavailable</div>
                    <div class="bm-unavail-legend">Unavailable</div>
                    <div class="bm-doublebook-legend">Double booked</div>
                  </div>
                </div>
                <div class="legend-hidden">
                  <div class="bm-legend-view hide" id="legend_view">View Legends</div>
                </div>
              </div>
            </div>

            <div class="bm-room-col">
              <div class="section meeting-type-section">
                <div class="section-header">
                  <span class="section-title" data-i18n="pick_meeting_type_and_room"></span>
                </div>
                <div class="section-tabs">
                  <ul class="nav nav-tabs hide" id="roomsTabList" role="tablist">
                    <li role="presentation" class="active" id="mappedTab">
                      <a href="#mappedroom" role="tab" data-roomType="mappedRooms" data-toggle="tab" aria-expanded="true" class="js_popover" data-placement="bottom" data-content="Rooms mapped to this meeting type">Recommended</a>
                    </li>
                    <li role="presentation" id="othersTab" class="others-tab-label">
                      <a href="#otherroom" role="tab" data-roomType="otherRooms" data-toggle="tab" aria-expanded="true" class="js_popover" data-placement="bottom" data-content="Rooms not mapped to this meeting type">Others</a>
                    </li>
                    <li role="presentation" id="offsiteTab" class="offsite-tab-label js-offsite-location">
                      <a href="#offsite" role="tab" data-roomType="offsiteLocation" data-toggle="tab" aria-expanded="true" data-i18n="add_offsite_location" class="js_popover" data-placement="bottom"></a>
                    </li>
                  </ul>
                </div>

                <div class="section-content">
                  <div class="tab-content tabs-attendees-content">
                    <div role="tabpanel" class="tab-pane active room-list-container" id="mappedroom">
                      <div class="meeting-list js_selected_room"></div>
                      <div class="room-select-list">
                        <div class="room-select-item select-room-btn booth-tour-item non-jmm-room js-selectNonJmmMstRoom hide" id="nonJmmRoom">
                          <div class="offsite-detail">
                            <div class="type-title text-ellipsis" id="nonJmmRoomName" data-i18n="mst_on_site_room"></div>
                          </div>
                        </div>

                        <div class="room-select-item select-room-btn booth-tour-item js_check_room js-selectBoothTour" id="boothTour">
                          <div class="offsite-detail">
                            <div class="type-title text-ellipsis" id="boothTourName"></div>
                            <div class="type-detail">
                              <span class="boothtour-address">ADDRESS</span>
                              <span id="boothTourAddress" class="boothtour-detail"></span>
                            </div>
                          </div>
                          <i class="jif-iris-boothtour booth-tour-icon js_tooltip" title="Booth tour" data-placement="left"></i>
                        </div>

                        <div class="room-list"></div>
                      </div>
                    </div>

                    <div role="tabpanel" class="tab-pane other-room-list-container" id="otherroom">
                      <div class="meeting-list"></div>
                      <div class="room-select-list">
                        <div class="room-list"></div>
                      </div>
                    </div>

                    <div role="tabpanel" class="tab-pane" id="offsite">
                      <div class="meeting-list"></div>
                      <div class="mst-offsite-check">
                        <div class="room-selected active mst-selected-offsite js-mst-toggle-offsite">
                          <div class="room-selected-icon hide">
                            <i class="jif-check"></i>
                          </div>
                          <div class="offsite-detail">
                            <div class="type-title text-ellipsis">Offsite</div>
                          </div>
                        </div>
                      </div>
                      <div class="room-select-list">
                        <div class="offsite-location js-offsite-location">
                          <form action="javascript:void(0)" class="jiffle-form">
                            <div class="form-group" data-form-i18n="location_name">
                              <label class="required">Location Name</label>
                              <input type="text" name="offsiteName" class="form-control" id="name" required="required" />
                            </div>
                            <div class="form-group" data-form-i18n="address">
                              <label>Details</label>
                              <textarea type="text" name="offsiteAddress" class="form-control" id="address"></textarea>
                            </div>
                            <div class="form-group text-right">
                              <a href="javascript:void(0)" class="btn btn-tertiary cancel hide" id='create-cancel-btn' data-i18n="CANCEL"></a>
                              <button class="btn blue-btn" id='add-offsite-btn' data-i18n="SAVE"></button>
                            </div>
                          </form>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Modal -->
  <!-- Do not give tabindex for select2 search to work. -->
  <div class="modal fade js-add-attendee-modal" type="internal" role="dialog" data-backdrop="static" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="content">
        <div class="jiffle-panel">
          <div class="jiffle-panel-head">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h1 data-i18n="add_attendee"></h1>
            <span class="hint" data-i18n="add_attendee_hint"></span>
          </div>
          <div class="jiffle-panel-body">
            <form action="javascript:void(0)" class="jiffle-form js-add-attendee-form" role="form">
              <div id="internalPrimaryFields"></div>
              <div id="internalAdditionalFormFields"></div>
              <div class="termsandcondition hide js-termsandcondition">
                <div class="checkbox">
                  <input type="checkbox" name="consent" id="internal_consent" value="true" />
                  <label for="internal_consent" class="add_user_consent_message"></label>
                </div>
              </div>
              <div class="form-group no-margin" data-form-i18n="add">
                <button type="button" class="btn btn-block btn-blue add-attendee-btn js-add_attendee">Add</button>
              </div>
            </form>
          </div>
          <div class="jiffle-panel-footer privacy-fw">
            <div class="foot-privacypolicy"></div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Do not give tabindex for select2 search to work. -->
  <div class="modal fade js-add-attendee-modal" type="external" role="dialog" data-backdrop="static" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="content">
        <div class="jiffle-panel">
          <div class="jiffle-panel-head">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h1 data-i18n="add_attendee"></h1>
            <span class="hint" data-i18n="add_attendee_hint"></span>
          </div>
          <div class="jiffle-panel-body">
            <form action="javascript:void(0)" class="jiffle-form js-add-attendee-form" role="form">
              <div class="form-group toggle-switch-m hide js-sync-contact-sfdc" id="js-external-sync-contact-sfdc">
                <div class="row">
                  <div class="col-xs-8">
                    <label class="js-sync-sfdc">Sync contact to SFDC</label>
                  </div>
                  <div class="col-xs-4">
                    <div class="toggle-switch">
                      <input type="checkbox" name="sync_contact" id="external_sync_sfdc_contact" />
                      <label for="external_sync_sfdc_contact"></label>
                    </div>
                  </div>
                </div>
              </div>
              <div id="externalPrimaryFields"></div>
              <div id="externalAdditionalFormFields"></div>
              <div class="termsandcondition hide js-termsandcondition">
                <div class="checkbox">
                  <input type="checkbox" name="consent" id="external_consent" value="true" />
                  <label for="external_consent" class="add_user_consent_message"></label>
                </div>
              </div>
              <div class="form-group no-margin" data-form-i18n="add">
                <button type="button" class="btn btn-lg btn-block btn-blue add-attendee-btn js-add_attendee" >Add</button>
              </div>
            </form>
          </div>
          <div class="jiffle-panel-footer privacy-fw">
            <div class="foot-privacypolicy"></div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Select time modal -->
  <div class="modal select-time-modal fade" id="select-time">
    <div class="modal-dialog">
      <div class="select-time">
        <h5 data-i18n="select_time"></h5>
        <form action="javascript:void(0)" class="jiffle-form">
          <div class="row">
            <div class="col-xs-6">
              <div class="form-group" data-form-i18n="start_time">
                <label>Start</label>
                <input type="text" class="form-control" placeholder="HH:MM" id="start-time" />
              </div>
            </div>
            <div class="col-xs-6">
              <div class="form-group" data-form-i18n="end_time">
                <label>End</label>
                <input type="text" class="form-control" placeholder="HH:MM" id="end-time" />
              </div>
            </div>
            <div class="col-xs-12">
              <div class="text-right">
                <a href="javascript:void(0)" class="cancel-" data-i18n="CANCEL"></a>
                <a href="javascript:void(0)" class="btn btn-blue" data-i18n="OK"></a>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>

  <!-- View attendee modal -->
  <div class="modal fade modal-user-info" id="attendee-information">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title" id="myModalLabel" data-i18n="basic_info"></h4>
        </div>
        <div class="modal-body">
        </div>
      </div>
    </div>
  </div>

  <div id='proceed_page' class='hide js-consecutive-meeting-proceed-page'>
    <div class="book-meeting page-pad-top page-pad-bottom bottom-margin-50">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-4 col-sm-6">
            <div class="section section-meeting-proceed">
              <div class="section-content">
                <div class="selected-item selected-time"></div>
                <div class="selected-item selected-room"></div>
                
                <div class="selected-item selected-internal-attendee no-padding-left-right">
                  <div class="selected-title" >
                    <div class="selected-title-left">
                      <i class="jif-user-circle-o jif-16"></i>
                      <span data-i18n="internal_participants"></span>
                      <span class="count selectedInternalAttendeeCount">0</span>
                    </div>
                    <div class="js-show-attendee">
                      <a href="javascript:void(0)" class='go-to-main-internal'>
                        <i class="jif-plus-circle-o"></i><span data-i18n="Add"></span>
                      </a>
                    </div>
                  </div>
                  <div class="selected-attendee-list scroll-y"></div>
                </div>

                <div class="selected-item selected-external-attendee no-padding-left-right">
                  <div class="selected-title">
                    <div class="selected-title-left">
                      <i class="jif-user-circle-o jif-16"></i>
                      <span data-i18n="external_participants"></span>
                      <span class="count selectedExternalAttendeeCount">0</span>
                    </div>
                    <div class="js-show-attendee">
