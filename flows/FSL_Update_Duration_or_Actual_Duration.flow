    </recordUpdates>
    <recordUpdates>
        <description>Update Duration in the Service Appointment record.</description>
        <name>Update_Duration</name>
        <label>Update Duration</label>
        <locationX>50</locationX>
        <locationY>551</locationY>
        <connector>
