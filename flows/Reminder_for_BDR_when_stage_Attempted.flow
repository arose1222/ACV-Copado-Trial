        <rules>
            <name>X2_Day_Task</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>TaskToBeCreated</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Create_Two_Day_Task</stringValue>
