            </connector>
            <label>2 Day Task</label>
        </rules>
    </decisions>
    <formulas>
        <name>TaskToBeCreated</name>
        <dataType>String</dataType>
        <expression>IF( ISNULL({!$Record.Follow_Up_Task_Created_On__c}) , &apos;Create_Two_Day_Task&apos; ,
IF ( (TODAY() - {!$Record.Follow_Up_Task_Created_On__c}) == 2 &amp;&amp; (TODAY() - DATEVALUE({!$Record.CreatedDate}))&lt;=10 , &apos;Create_Two_Day_Task&apos; ,
IF( (TODAY() - {!$Record.Follow_Up_Task_Created_On__c}) &gt;= 7 &amp;&amp; (TODAY() - DATEVALUE({!$Record.CreatedDate}))&gt;10 , &apos;Create_Weekly_Task&apos; , &apos;&apos; ) ) )</expression>
    </formulas>
    <formulas>
        <name>TodayPlus2</name>
        <dataType>Date</dataType>
        <expression>TODAY() + 2</expression>
    </formulas>
    <formulas>
        <name>TodayPlus7</name>
